<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    tools:context="com.development.rayout.menuichi.MenuPrincipalActivity">

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="0dp"
        android:layout_height="320dp"
        android:layout_marginEnd="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
      android:id="@+id/relativeLayout2"
      android:layout_width="0dp"
      android:layout_height="320dp"
      android:layout_marginBottom="0dp"
      android:background="#90000000"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"

      app:layout_constraintTop_toTopOf="parent">

    </RelativeLayout>

    <ImageView
      android:id="@+id/imageView4"
      android:layout_width="483dp"
      android:layout_height="141dp"
      android:layout_marginTop="80dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/ichiban_splash_white"
      tools:layout_editor_absoluteY="80dp"/>

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:alpha="0.7"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout2">

    </RelativeLayout>

    <android.support.v7.widget.CardView
      android:id="@+id/appetizer_card"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:layout_marginStart="48dp"
      android:layout_marginEnd="270dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:clickable="true"
      android:focusable="true"
      android:foreground="?android:attr/selectableItemBackground"
      app:cardCornerRadius="60dp"
      app:cardElevation="@dimen/activity_vertical_margin"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/relativeLayout2"
      tools:layout_editor_absoluteX="16dp">

        <android.support.constraint.ConstraintLayout
          android:layout_width="match_parent"
          android:layout_height="match_parent">

            <ImageView
              android:id="@+id/imageView"
              android:layout_width="150dp"
              android:layout_height="120dp"
              android:layout_marginStart="0dp"
              android:adjustViewBounds="false"
              android:cropToPadding="true"
              android:scaleType="centerCrop"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent"
              app:layout_constraintVertical_bias="0.0"
              app:srcCompat="@drawable/thumbnail"/>

            <TextView
              android:id="@+id/textView3"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/open_sans_semibold"
              android:text="APPETIZERS"
              android:textSize="24sp"
              android:visibility="visible"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintHorizontal_bias="0.7"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent"
              tools:layout_editor_absoluteY="48dp"/>

            <ImageView
              android:id="@+id/imageView11"
              android:layout_width="180dp"
              android:layout_height="120dp"
              android:layout_marginStart="0dp"
              android:visibility="gone"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintHorizontal_bias="0.65"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent"
              app:srcCompat="@drawable/appetizer_title"
              tools:layout_editor_absoluteX="224dp"
              tools:layout_editor_absoluteY="13dp"/>
        </android.support.constraint.ConstraintLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView

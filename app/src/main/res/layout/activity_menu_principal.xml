              app:layout_constraintTop_toTopOf="parent"
              tools:layout_editor_absoluteX="253dp"
              tools:layout_editor_absoluteY="51dp"/>
        </android.support.constraint.ConstraintLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
